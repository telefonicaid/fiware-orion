# VALGRIND_READY - to mark the test ready for valgrindTestSuite.sh

--NAME--
String filters for compound values of attributes: greater_than

--SHELL-INIT--
dbInit CB
brokerStart CB 0-255

--SHELL--

#
# 01. Create E1 with A1 with compound value containing all JSON types
#
# FIXME P5: GT/GE only support strings and numbers, once more types are supported, add test cases here
#
# 02. Integer GT is true: GET /v2/entities?q=A1.i>9, see E1
# 03. Integer GT is true: GET /v2/entities?q=A1.i>-1, see E1
# 04. Integer GT is false: GET /v2/entities?q=A1.i>10, see nothing
# 05. Integer GE is true: GET /v2/entities?q=A1.i>=9, see E1
# 06. Integer GE is true: GET /v2/entities?q=A1.i>=-1, see E1
# 07. Integer GE is true: GET /v2/entities?q=A1.i>=10, see E1
# 08. Integer GE is false: GET /v2/entities?q=A1.i>=11, see nothing
#
# 09. Float GT is true: GET /v2/entities?q=A1.f>2, see E1
# 10. Float GT is true: GET /v2/entities?q=A1.f>-1, see E1
# 11. Float GT is false: GET /v2/entities?q=A1.f>10, see nothing
# 12. Float GE is true: GET /v2/entities?q=A1.f>=3.0, see E1
# 13. Float GE is true: GET /v2/entities?q=A1.f>=-1.0, see E1
# 14. Float GE is true: GET /v2/entities?q=A1.f>=3.14, see E1
# 15. Float GE is false: GET /v2/entities?q=A1.f>=3.15, see nothing
#
# 16. String GT is true: GET /v2/entities?q=A1.s>fest_string, see E1
# 17. String GT is false: GET /v2/entities?q=A1.s>uest_string, see nothing
# 18. String GE is true: GET /v2/entities?q=A1.s>=fest_string, see E1
# 19. String GE is true: GET /v2/entities?q=A1.s>=test_string, see E1
# 20. String GE is false: GET /v2/entities?q=A1.s>=uest_string, see nothing
#

echo "01. Create E1 with A1 with compound value containing all JSON types"
echo "==================================================================="
payload='{
  "id": "E1",
  "A1": {
    "value": {
      "i": 10,
      "f": 3.14,
      "s": "test_string",
      "T": true,
      "F": false,
      "N": null
    }
  },
  "A2": { "value": 22 }
}'
orionCurl --url /v2/entities --payload "${payload}"
echo
echo


echo "02. Integer GT is true: GET /v2/entities?q=A1.i>9, see E1"
echo "========================================================="
orionCurl --url '/v2/entities?q=A1.i>9'
echo
echo


echo "03. Integer GT is true: GET /v2/entities?q=A1.i>-1, see E1"
echo "=========================================================="
orionCurl --url '/v2/entities?q=A1.i>-1'
echo
echo


echo "04. Integer GT is false: GET /v2/entities?q=A1.i>10, see nothing"
echo "================================================================"
orionCurl --url '/v2/entities?q=A1.i>10'
echo
echo


echo "05. Integer GE is true: GET /v2/entities?q=A1.i>=9, see E1"
echo "=========================================================="
orionCurl --url '/v2/entities?q=A1.i>=9'
echo
echo


echo "06. Integer GE is true: GET /v2/entities?q=A1.i>=-1, see E1"
echo "==========================================================="
orionCurl --url '/v2/entities?q=A1.i>=-1'
echo
echo


echo "07. Integer GE is true: GET /v2/entities?q=A1.i>=10, see E1"
echo "==========================================================="
orionCurl --url '/v2/entities?q=A1.i>=10'
echo
echo


echo "08. Integer GE is false: GET /v2/entities?q=A1.i>=11, see nothing"
echo "================================================================="
orionCurl --url '/v2/entities?q=A1.i>=11'
echo
echo


echo "09. Float GT is true: GET /v2/entities?q=A1.f>2, see E1"
echo "======================================================="
orionCurl --url '/v2/entities?q=A1.f>2'
echo
echo


echo "10. Float GT is true: GET /v2/entities?q=A1.f>-1, see E1"
echo "========================================================"
orionCurl --url '/v2/entities?q=A1.f>-1'
echo
echo


echo "11. Float GT is false: GET /v2/entities?q=A1.f>10, see nothing"
echo "=============================================================="
orionCurl --url '/v2/entities?q=A1.f>10'
echo
echo


echo "12. Float GE is true: GET /v2/entities?q=A1.f>=3.0, see E1"
echo "=========================================================="
orionCurl --url '/v2/entities?q=A1.f>=3.0'
echo
echo


echo "13. Float GE is true: GET /v2/entities?q=A1.f>=-1.0, see E1"
echo "==========================================================="
orionCurl --url '/v2/entities?q=A1.f>=-1.0'
echo
echo


echo "14. Float GE is true: GET /v2/entities?q=A1.f>=3.14, see E1"
echo "==========================================================="
orionCurl --url '/v2/entities?q=A1.f>=3.14'
echo
echo


echo "15. Float GE is false: GET /v2/entities?q=A1.f>=3.15, see nothing"
echo "================================================================="
orionCurl --url '/v2/entities?q=A1.f>=3.15'
echo
echo


echo "16. String GT is true: GET /v2/entities?q=A1.s>fest_string, see E1"
echo "=================================================================="
orionCurl --url '/v2/entities?q=A1.s>fest_string'
echo
echo


echo "17. String GT is false: GET /v2/entities?q=A1.s>uest_string, see nothing"
echo "========================================================================"
orionCurl --url '/v2/entities?q=A1.s>uest_string'
echo
echo


echo "18. String GE is true: GET /v2/entities?q=A1.s>=fest_string, see E1"
echo "==================================================================="
orionCurl --url '/v2/entities?q=A1.s>=fest_string'
echo
echo


echo "19. String GE is true: GET /v2/entities?q=A1.s>=test_string, see E1"
echo "==================================================================="
orionCurl --url '/v2/entities?q=A1.s>=test_string'
echo
echo


echo "20. String GE is false: GET /v2/entities?q=A1.s>=uest_string, see nothing"
echo "========================================================================="
orionCurl --url '/v2/entities?q=A1.s>=uest_string'
echo
echo


