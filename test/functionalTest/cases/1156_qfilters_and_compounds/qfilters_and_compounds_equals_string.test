# VALGRIND_READY - to mark the test ready for valgrindTestSuite.sh

--NAME--
String filters for compound values of attributes: equality of float

--SHELL-INIT--
dbInit CB
brokerStart CB

--SHELL--

#
# 01. Create E1 with A1 with compound value containing all JSON types
#
# 02. Equal to Same String: GET /v2/entities?q=A1.s:test_string, see E1
# 03. Equal to Same String: GET /v2/entities?q=A1.s==test_string, see E1
# 04. Equal to Different String: GET /v2/entities?q=A1.s:other_string, see nothing
# 05. Equal to Different String: GET /v2/entities?q=A1.s==other_string, see nothing
# 06. Not Equal to Same String: GET /v2/entities?q=A1.s!=test_string, see nothing
# 07. Not Equal to Different String: GET /v2/entities?q=A1.s!=other_string, see E1
# 08. Equal to Incorrect String-Range: GET /v2/entities?q=A1.s:a_string..s_string, see nothing
# 09. Equal to Correct String-Range: GET /v2/entities?q=A1.s:lo_string..test_string, see E1
# 10. Not Equal to Incorrect String-Range: GET /v2/entities?q=A1.s!=a_string..s_string, see E1
# 11. Not Equal to Correct String-Range: GET /v2/entities?q=A1.s!=test_string..z_string, see nothing
# 12. Equal to Incorrect String-List: GET /v2/entities?q=A1.s:S1,S2,test_strings, see nothing
# 13. Equal to Correct String-List: GET /v2/entities?q=A1.s:S1,S2,test_string, see E1
# 14. Not Equal to Incorrect String-List: GET /v2/entities?q=A1.s!=test_strings,S1,S2, see E1
# 15. Not Equal to Correct String-List: GET /v2/entities?q=A1.s!=test_string,S1,S2, see nothing
#


echo "01. Create E1 with A1 with compound value containing all JSON types"
echo "==================================================================="
payload='{
  "id": "E1",
  "A1": {
    "value": {
      "i": 10,
      "f": 3.14,
      "s": "test_string",
      "T": true,
      "F": false,
      "N": null,
      "D": "2016-07-22T10:42:42.000Z"
    }
  },
  "A2": { "value": 22 }
}'
orionCurl --url /v2/entities --payload "${payload}"
echo
echo


echo "02. Equal to Same String: GET /v2/entities?q=A1.s:test_string, see E1"
echo "====================================================================="
orionCurl --url '/v2/entities?q=A1.s:test_string'
echo
echo


echo "03. Equal to Same String: GET /v2/entities?q=A1.s==test_string, see E1"
echo "======================================================================"
orionCurl --url '/v2/entities?q=A1.s==test_string'
echo
echo


echo "04. Equal to Different String: GET /v2/entities?q=A1.s:other_string, see nothing"
echo "================================================================================"
orionCurl --url '/v2/entities?q=A1.s:other_string'
echo
echo


echo "05. Equal to Different String: GET /v2/entities?q=A1.s==other_string, see nothing"
echo "================================================================================="
orionCurl --url '/v2/entities?q=A1.s==other_string'
echo
echo


echo "06. Not Equal to Same String: GET /v2/entities?q=A1.s!=test_string, see nothing"
echo "==============================================================================="
orionCurl --url '/v2/entities?q=A1.s!=test_string'
echo
echo


echo "07. Not Equal to Different String: GET /v2/entities?q=A1.s!=other_string, see E1"
echo "================================================================================"
orionCurl --url '/v2/entities?q=A1.s!=other_string'
echo
echo


echo "08. Equal to Incorrect String-Range: GET /v2/entities?q=A1.s:a_string..s_string, see nothing"
echo "============================================================================================"
orionCurl --url '/v2/entities?q=A1.s:a_string..s_string'
echo
echo


echo "09. Equal to Correct String-Range: GET /v2/entities?q=A1.s:lo_string..test_string, see E1"
echo "========================================================================================="
orionCurl --url '/v2/entities?q=A1.s:lo_string..test_string'
echo
echo


echo "10. Not Equal to Incorrect String-Range: GET /v2/entities?q=A1.s!=a_string..s_string, see E1"
echo "============================================================================================"
orionCurl --url '/v2/entities?q=A1.s!=a_string..s_string'
echo
echo


echo "11. Not Equal to Correct String-Range: GET /v2/entities?q=A1.s!=test_string..z_string, see nothing"
echo "=================================================================================================="
orionCurl --url '/v2/entities?q=A1.s!=test_string..z_string'
echo
echo


echo "12. Equal to Incorrect String-List: GET /v2/entities?q=A1.s:S1,S2,test_strings, see nothing"
echo "==========================================================================================="
orionCurl --url '/v2/entities?q=A1.s:S1,S2,test_strings'
echo
echo


echo "13. Equal to Correct String-List: GET /v2/entities?q=A1.s:S1,S2,test_string, see E1"
echo "==================================================================================="
orionCurl --url '/v2/entities?q=A1.s:S1,S2,test_string'
echo
echo


echo "14. Not Equal to Incorrect String-List: GET /v2/entities?q=A1.s!=test_strings,S1,S2, see E1"
echo "==========================================================================================="
orionCurl --url '/v2/entities?q=A1.s!=test_strings,S1,S2'
echo
echo


echo "15. Not Equal to Correct String-List: GET /v2/entities?q=A1.s!=test_string,S1,S2, see nothing"
echo "============================================================================================="
orionCurl --url '/v2/entities?q=A1.s!=test_string,S1,S2'
echo
echo


