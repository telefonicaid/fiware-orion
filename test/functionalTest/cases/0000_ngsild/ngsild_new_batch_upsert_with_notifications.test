echo "05. GET all entities and see the merged E1 and E2"
echo "================================================="
orionCurl --url /ngsi-ld/v1/entities?type=T
echo
echo


echo "06. Dump accumulator, see 2 notifications, with 6 entities each"
echo "==============================================================="
accumulatorDump
accumulatorReset
echo
echo


