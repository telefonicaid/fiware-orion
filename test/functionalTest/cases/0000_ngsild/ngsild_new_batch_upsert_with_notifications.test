    "notificationOrder": 8
  },
  {
    "id": "urn:E1",
    "type": "T",
    "P3": "step 4.3",
    "P4": "step 4.3",
    "R3": { "object": "urn:E3" },
    "R4": { "object": "urn:E3" },
