echo "01. Create entity E1 with atribute A with metadata M1 and M2"
echo "============================================================"
payload='{
  "id": "E1",
  "type": "T",
  "A": {
    "value": "foo",
    "metadata": {
      "M1": {
        "value": "bar"
      },
      "M2": {
        "value": "muu"
      }
    }
  }
}'
orionCurl --url '/v2/entities' --payload "$payload"
echo
echo


echo "02a. GET /v2/entities?metadata=dateCreated"
echo "=========================================="
orionCurl --url '/v2/entities?metadata=dateCreated'
