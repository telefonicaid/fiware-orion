echo "=============================================================================================================================================="
payload='[
  {
    "id": "urn:ngsi-ld:entity:E1",
    "type": "Device",
    "location": {
      "type": "GeoProperty",
      "value": {
        "type": "Point",
        "coordinates": [ 1, 2 ]
      }
    },
    "createdAt": "2021-03-07T08:30:00.123Z",
    "modifiedAt": "2021-03-07T08:30:00.123Z",
    "observationSpace": {
      "type": "GeoProperty",
      "value": {
        "type": "Point",
        "coordinates": [ 1, 3 ]
      }
    },
    "operationSpace": {
      "type": "GeoProperty",
      "value": {
        "type": "Point",
        "coordinates": [ 1, 4 ]
      }
    },
    "weight": {
      "type":  "Property",
      "value": 250,
      "unitCode": "kg",
      "observedAt": "2021-03-07T08:31:00.123Z"
    },
    "owner": {
      "type":  "Relationship",
      "object": "urn:ngsi-ld:owner:1",
      "observedAt": "2021-03-07T08:32:00.123Z"
    }
  }
]'
orionCurl --url /ngsi-ld/v1/entityOperations/create --payload "$payload"
