# VALGRIND_READY - to mark the test ready for valgrindTestSuite.sh

--NAME--
Exhaustive test of adding attributes to an entity - attrs are not overwritten (options=noOverwrite)

--SHELL-INIT--
export BROKER=orionld
dbInit CB
dbInit CB t1
pgInit $CB_DB_NAME
pgInit ${CB_DB_NAME}_t1
brokerStart CB 100 IPv4 -troe -multiservice

--SHELL--

