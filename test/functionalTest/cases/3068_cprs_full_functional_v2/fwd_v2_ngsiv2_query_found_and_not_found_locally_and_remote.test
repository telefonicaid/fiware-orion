# 19. Query E1/A1
#

echo "01. Register E1/T1/A1+A2    in CB, provApp: CP1"
echo "==============================================="
payload='{
  "dataProvided": {
    "entities": [
      {
        "type": "T1",
        "id": "E1"
      }
    ],
    "attrs": [ "A1", "A2" ]
  },
  "provider": {
    "http": {
      "url": "http://localhost:'${CP1_PORT}'/v2"
    }
  }
}'
